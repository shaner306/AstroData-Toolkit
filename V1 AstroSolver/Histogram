import matplotlib.pyplot as plt
import numpy as np

f=open("/Users/home/Downloads/Data Product.txt","r")

result=[]
residuals=[]
for x in f:
    result.append(x.split())

for x in range(0,len(result)):
    if x >= 7:
        if result[x][7]=="Right":
            residuals.append(result[x][9])
        else:
            residuals.append(result[x][8])
print(residuals)


